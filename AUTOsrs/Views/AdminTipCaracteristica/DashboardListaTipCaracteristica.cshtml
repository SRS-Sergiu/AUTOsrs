@model IEnumerable<AUTOsrs.Models.TipCaracteristicaModel>


@{
    ViewBag.Title = "Lista Tip Caracteristici";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Lista Marci</h2>

<p>
    @Html.ActionLink("Create New", "DashboardCreareTipCaracteristica")
</p>
<table class="table asd">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NumeTipCaracteristica)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.NumeTipCaracteristica)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditTipCaracteristica", new { id = item.ID_TipCaracteristica }) |
                @Html.ActionLink("Delete", "DeleteTipCaract", new { id = item.ID_TipCaracteristica })
            </td>
        </tr>
    }

</table>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}